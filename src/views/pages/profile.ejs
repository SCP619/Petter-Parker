<%- include('../partials/metadata') %>
    <%- include('../partials/navbar') %>

        <main class="container text-center h-100">
            <div class="row">
                <div class="col">

                </div>
                <div class="col d-flex align-items-center vh-100">
                    <div class="card" style="width: 180rem; height: 50%;">
                        <table class="table table-borderless">
                            <thead class="table-dark">
                                <tr>
                                    <th colspan="2">Account Information</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td align="left">
                                        <h4>
                                            UserName
                                        </h4>
                                    </td>
                                    <td align="right">
                                        <%= name %>
                                    </td>
                                </tr>
                                <tr>
                                    <td align="left">
                                        <h4>Email Address</h4>
                                    </td>
                                    <td align="right">
                                        <%= email_address %>
                                    </td>
                                </tr>
                                <tr>
                                    <td align="left">
                                        <h4>Account Type</h4>
                                    </td>
                                    <td align="right">
                                        <%= type %>
                                    </td>
                                </tr>
                                <tr>
                                    <td align="left">
                                        <h4>Location</h4>
                                    </td>
                                    <td align="right">
                                        <%= location %>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                        <div class="vstack gap-2 col-md-5 mx-auto">
                            <button type="button" class="btn btn-secondary btn-success">See Parking Details</button>
                        </div>
                    </div>
                </div>
                <div class="col">

                </div>
            </div>
        </main>
        <%- include('../partials/close') %>